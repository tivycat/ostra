{
  "firestore": {
    "rules": "firestore.rules"
  },
  "hosting": {
    "site" : "ostragymnasiet",
    "public": "./public",
    "ignore": [
      "db.js"
    ],
    "headers": [{
      "source" : "**/*.@(js|html)",
      "headers" : [ {
        "key" : "Cache-Control",
        "value" : "max-age=0"
      } ]
    }]
  }
}